{
  "name": "zod-prisma",
  "version": "0.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@egoist/prettier-config",
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "zod-prisma": "dist/cli.js"
  },
  "scripts": {
    "build-fast": "tsup-node src/cli.ts --format cjs,esm",
    "build": "pnpm run build-fast",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky install",
    "lint": "tsc --noEmit && eslint vitest.config.ts src/*.ts test/*.test.ts test/**/*.test.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@prisma/client": "^3.8.1",
    "@prisma/sdk": "^3.8.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "decimal.js": "^10.3.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "6.0.0",
    "fast-glob": "3.2.11",
    "fs-jetpack": "4.3.1",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "prisma": "^3.8.1",
    "tsup": "5.11.13",
    "typescript": "4.5.4",
    "vite": "^2.7.13",
    "vite-plugin-eslint": "^1.3.0",
    "vitest": "0.2.5",
    "zod": "^3.11.6"
  },
  "dependencies": {
    "@prisma/generator-helper": "^3.8.1",
    "parenthesis": "3.1.8",
    "ts-morph": "13.0.3"
  },
  "peerDependencies": {
    "decimal.js": "^10.0.0",
    "prisma": "^3.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "decimal.js": {
      "optional": true
    }
  }
}